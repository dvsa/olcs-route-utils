{
    "name": "dvsa/olcs-route-utils",
    "description": "A package to provide utils used for dynamic routing and or creating router config in Zend",
    "type": "library",
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "mockery/mockery": "^1.2"
    },
    "license": "MIT Licence",
    "authors": [
        {
            "name": "Shaun Hare",
            "email": "shaun.hare@dvsa.gov.uk"
        },
        {
            "name": "Igor Stimoli",
            "email": "igor.stimoli@bjss.com"
        }
    ],
    "minimum-stability": "stable",
    "require": {
        "php": ">=7.1"
    },
    "autoload": {
        "psr-4": {
            "Dvsa\\Olcs\\RouteUtils\\": "src/",
            "Dvsa\\Olcs\\Test\\": "test/"
        }
    },
    "config": {
        "preferred-install": "dist",
        "secure-http" : false,
        "platform": {
            "php": "7.1.16"
        }
    }
}
